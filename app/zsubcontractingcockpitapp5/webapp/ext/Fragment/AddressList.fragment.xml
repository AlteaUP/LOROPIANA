<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectAddressListDialog"
		title="{i18n>addressListTitle}"
		items="{address_model>/addressListSet}"
		search=".onValueHelpAddressListSearch"
		confirm=".onValueHelpAddressListConfirm"
		cancel=".onValueHelpAddressListClose">
		<StandardListItem
			title="{address_model>Name1}"
			description="{address_model>indirizzo}"/>
	</SelectDialog>
</core:FragmentDefinition>